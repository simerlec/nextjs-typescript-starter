{
  "name": "react-ts-starter",
  "version": "1.0.0",
  "description": "react ts starter",
  "main": "index.js",
  "author": "@simerlec",
  "license": "MIT",
  "scripts": {
    "build": "parcel build src/index.html --out-dir public",
    "dev": "parcel src/index.html",
    "format": "prettier --config ../.prettierrc --write --list-different \"src/**/*.{ts,tsx,css,html}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --quiet",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.12.0"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,json,html,css,scss,md}": [
      "prettier --config .prettierrc --write"
    ]
  }
